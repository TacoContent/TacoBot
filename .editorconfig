
root = true

[*]
charset = utf-8
indent_style = space
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_size = 2

[*.{json,yml,yaml,manifest}]
indent_style = space
indent_size = 2

[.vscode/settings.json]
indent_style = tab

[*.py]
indent_style = space
indent_size = 4

[*.md]
trim_trailing_whitespace = false
insert_final_newline = true

[tacobot.egg-info/*]
insert_final_newline = unset
trim_trailing_whitespace = unset
end_of_line = unset
charset = unset
indent_style = unset
indent_size = unset
